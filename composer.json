{
  "name": "rafgirao/short-url",
  "description": "A Laravel package for creating shortened URLs for your web apps.",
  "type": "library",
  "homepage": "https://github.com/rafgirao/short-url",
  "license": "MIT",
  "authors": [
    {
      "name": "Rafael Girao",
      "email": "rgiraon@gmail.com"
    }
  ],
  "keywords": [
    "rafaelgirao",
    "short-urls",
    "url-shortener",
    "laravel",
    "laravel-package"
  ],
  "require": {
    "php": "^7.2|^8.0",
    "nesbot/carbon": "~2.0",
    "illuminate/container": "^6.0|^7.0|^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0",
    "jenssegers/agent": "^2.6",
    "hashids/hashids": "^4.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.2"
  },
  "autoload": {
    "psr-4": {
      "RafaelGirao\\ShortURL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RafaelGirao\\ShortURL\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RafaelGirao\\ShortURL\\Providers\\ShortURLProvider"
      ],
      "aliases": {
        "ShortURL": "RafaelGirao\\ShortURL\\Facades\\ShortURL"
      }
    }
  }
}
